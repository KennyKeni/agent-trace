{
  "version": 1,
  "hooks": {
    "sessionStart": [
      {
        "command": "bunx @kennykeni/agent-trace hook --provider cursor"
      }
    ],
    "sessionEnd": [
      {
        "command": "bunx @kennykeni/agent-trace hook --provider cursor"
      }
    ],
    "beforeSubmitPrompt": [
      {
        "command": "bunx @kennykeni/agent-trace hook --provider cursor"
      }
    ],
    "afterFileEdit": [
      {
        "command": "bunx @kennykeni/agent-trace hook --provider cursor"
      }
    ],
    "afterTabFileEdit": [
      {
        "command": "bunx @kennykeni/agent-trace hook --provider cursor"
      }
    ],
    "afterShellExecution": [
      {
        "command": "bunx @kennykeni/agent-trace hook --provider cursor"
      }
    ],
    "beforeShellExecution": [
      {
        "command": "bunx @kennykeni/agent-trace hook --provider cursor"
      }
    ]
  }
}
